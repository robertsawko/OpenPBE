FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      S0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 919450818;

boundaryField
{
    outlet
    {
        type               inletOutlet;
        inletValue         $internalField;
        value              $internalField;

    }
    walls
    {
        type            zeroGradient;
    }
    oilInlet
    {
        type            fixedValue;
        value           $internalField;
    }
    waterInlet
    {
        type            fixedValue;
        value           $internalField;
    }

}


