FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiphaseEulerFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2;

deltaT          1e-04;//3e-04;//5e-05;//0.00005;

writeControl    runTime;//timeStep;//runTime;//timeStep;//adjustableRunTime;

writeInterval   0.05;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   10;

runTimeModifiable on;

adjustTimeStep  no;

maxCo           0.25;

maxDeltaT       1e-02;

libs ("libPBE.so");
// ************************************************************************* //
